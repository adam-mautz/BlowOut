@model BlowOut.Models.InstrumentClient
@{
    ViewBag.Title = "ClientInstrument";
}

<table style="width: 100%">
    <caption>Clients</caption>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
    </tr>
    @foreach (var client in Model.clients)
    {
        <tr>
            <td>@client.Id</td>
            <td>@client.First_Name</td>
            <td>@client.Last_Name</td>
            <td>@client.address</td>
            <td>@client.city</td>
            <td>@client.state</td>
            <td>@client.zip</td>
        </tr>
    }
</table>

<br />

<table style="width: 100%">
    <caption style="display: table-caption; text-align: center;">Instruments</caption>
    <tr>
        <th>Id</th>
        <th>Description</th>
        <th>Type</th>
        <th>Price</th>
        <th>Client Id</th>

    </tr>
    @foreach (var instrumet in Model.instruments)
    {
        <tr>
            <td>@instrumet.Id</td>
            <td>@instrumet.description</td>
            <td>@instrumet.type</td>
            <td>@instrumet.price</td>
            <td>@instrumet.Client_ID</td>
        </tr>
    }
</table>

